#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules/typescript/bin/tsserver/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules/typescript/bin/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules/typescript/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/node_modules:/Users/yhawin/AppSourceCode/SourceCode/node_modules:/Users/yhawin/AppSourceCode/node_modules:/Users/yhawin/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../typescript/3.8.3/node_modules/typescript/bin/tsserver" "$@"
  ret=$?
else 
  NODE_PATH="/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules/typescript/bin/tsserver/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules/typescript/bin/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules/typescript/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/3.8.3/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules/.pnpm/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/siapp/node_modules:/Users/yhawin/AppSourceCode/SourceCode/Git-Workspace/node_modules:/Users/yhawin/AppSourceCode/SourceCode/node_modules:/Users/yhawin/AppSourceCode/node_modules:/Users/yhawin/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../typescript/3.8.3/node_modules/typescript/bin/tsserver" "$@"
  ret=$?
